{
  "name": "Holb Checker thing 2.0",
  "version": "1.0",
  "description": "This will hopefully do sometign with the DOM",
  "manifest_version": 2,
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
		{
			"matches": ["https://intranet.hbtn.io/projects/*"],
			"js": ["content.js"]
		}
	],
	
	"icons": {
		"16": "images/holb_logo16.png",
		"32": "images/holb_logo32.png",
		"48": "images/holb_logo48.png",
		"128": "images/holb_logo128.png"
  }
}
